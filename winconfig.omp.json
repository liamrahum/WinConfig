{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#97DFFC",
          "style": "plain",
          "template": "\u250f[{{ .UserName }}]",
          "type": "session"
        },
        {
          "foreground": "#97DFFC",
          "style": "plain",
          "template": "[<#D9DBF1>\uf0e7</>]",
          "type": "root"
        },
        {
          "foreground": "#97DFFC",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "[<#D9DBF1>{{ .Path }}</>]",
          "type": "path"
        },
        {
          "foreground": "#D9DBF1",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#97DFFC>[</><#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#97DFFC>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\ueb4b</> {{ .StashCount }}{{ end }}<#97DFFC>]</>",
          "type": "git"
        },
        {
          "foreground": "#D9DBF1",
          "style": "plain",
          "template": "[{{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }}]",
          "type": "aws"
        },
        {
          "foreground": "#D9DBF1",
          "style": "plain",
          "template": "[{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "#97DFFC",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "[<#ffffff>\uf252</> {{ .FormattedMs }}s]",
          "type": "executiontime"
        },
        {
          "foreground": "#ff4949",
          "style": "powerline",
          "template": "[<#ffffff>\uea6c Error</>]",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#97DFFC",
          "style": "plain",
          "template": "\u2514\u2500î˜‚",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
